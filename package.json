  "restify": "~8.5.1",
  "serialize-instance": "^1.0.5",
  "winston": "^3.3.3"
 },
 "devDependencies": {
  "@commitlint/cli": "^11.0.0",
  "@commitlint/config-conventional": "^11.0.0",
  "@types/knex": "^0.14.26",
